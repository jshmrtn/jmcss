{
  "name": "jmscss",
  "version": "1.2.0",
  "description": "JM SCSS generator.",
  "main": "src/index.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "zem start",
    "build": "npm run clean && zem build",
    "test": "npm run tslint && npm run jest",
    "clean": "zem clean",
    "exec": "zem exec",
    "deploy": "now -n jm-scss-zeplin-extension --scope joshmartin dist && now ln --scope joshmartin jm-scss-zeplin-extension-jeremyzahner.joshmartin1.now.sh https://zeplin.joshmartin.rocks",
    "jest": "NODE_ENV=jest ./node_modules/.bin/jest",
    "tslint": "tslint 'src/**/*.ts?(x)' --project ."
  },
  "zeplin": {
    "displayName": "JMCSS",
    "platforms": [
      "web"
    ],
    "options": [
      {
        "name": "Split Symbol",
        "type": "text",
        "id": "splitSymbol",
        "default": "/"
      },
      {
        "name": "Default: color",
        "type": "text",
        "id": "defaultColor",
        "default": "initial"
      },
      {
        "name": "Default: font-weight",
        "type": "text",
        "id": "defaultFontWeight",
        "default": "normal"
      },
      {
        "name": "Default: font-style",
        "type": "text",
        "id": "defaultFontStyle",
        "default": "normal"
      },
      {
        "name": "Default: text-transform",
        "type": "text",
        "id": "defaultTextTransform",
        "default": "none"
      },
      {
        "name": "Default: text-align",
        "type": "text",
        "id": "defaultTextAlign",
        "default": "left"
      },
      {
        "name": "Use relative units (only for web projects)",
        "type": "switch",
        "id": "relativeUnits",
        "default": false
      },
      {
        "name": "Relative unit base length in px",
        "type": "text",
        "id": "relativeUnitBaseLength",
        "default": "16"
      },
      {
        "name": "Media query mixin name",
        "type": "text",
        "id": "mediaQueryMixinName",
        "default": "media-breakpoint-up"
      },
      {
        "name": "Show variables with default values",
        "type": "switch",
        "id": "includeDefaultValue",
        "default": true
      },
      {
        "name": "Standard Subfix",
        "type": "text",
        "id": "standardSubfix",
        "default": "std"
      }
    ]
  },
  "dependencies": {
    "nunjucks": "^3.2.0",
    "zem": "^0.4.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/nunjucks": "^3.1.1",
    "ejs": "^2.6.1",
    "jest": "^24.8.0",
    "jest-multiline-matchers": "^0.2.1",
    "jest-nunjucks": "^1.0.0",
    "nunjucks-loader": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  }
}
